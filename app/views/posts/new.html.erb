<h1>Post Something New!</h1>
<br><br>

<%= form_for @post  do |f| %>
  <div class="field">
    <%= f.label :file_name %><br />
    <%= f.file_field :file_name, :class => "my-form" %>
  </div>

<br>
  
  <div class="field">
    <%= f.label :Content %><br />
    <%= f.text_area :content, :class => "form-control my-form" %>
  </div>  

<br>
  <div class="actions">
    <%= f.submit "Post!", :class => "btn btn-default" %>
  </div>
<% end%>
