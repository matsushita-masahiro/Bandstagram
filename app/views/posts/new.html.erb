<h1>Post Something New!</h1>
<br><br>

<%= form_for @post, :html => { multipart: true } do |f| %>


  <div class="field">
    <%= f.label :file_name %><br />
    <ul>
      <li>ファイル複数選択：<%= file_field_tag "images[]", type: :file, multiple: true, :class => "my-form" %></li>
    </ul>
  </div>

<br>

  <div class="field">
    <%= f.label :Content %><br />
    <%= f.text_area :content, :class => "form-control my-form" %>
  </div>  

<br>
  <div class="actions">
    <%= f.submit "Post!", :class => "btn btn-default" %>
  </div>
<% end%>
